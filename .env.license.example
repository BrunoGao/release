# LJWX 许可证环境配置
# 复制此文件为 .env 并填写实际值

# 数据库配置
MYSQL_ROOT_PASSWORD=your_mysql_root_password
MYSQL_DATABASE=ljwx
MYSQL_USER=ljwx
MYSQL_PASSWORD=your_mysql_password

# Redis配置
REDIS_PASSWORD=your_redis_password

# 许可证监控配置（可选）
ALERT_EMAIL=admin@yourdomain.com
ALERT_WEBHOOK=https://your-webhook-url.com/license-alert

# 其他配置
MYSQL_HOST=mysql
REDIS_HOST=redis

# 许可证功能配置
LJWX_LICENSE_ENABLED=true
LJWX_LICENSE_TRIAL_MODE=false  # 生产环境设为 false