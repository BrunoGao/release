.indoor,
.outdoor {
    display: inline-block;
    color: white;
    text-indent: 8px;
    margin-left: 8px;
}

.indoor {
    border-left: 4px solid #B0CC35;
}

.outdoor {
    border-left: 4px solid red;
}

/*temp-k*/
.temp-k {
    height: 21%;
    margin-top: 3%;
}

#tempKChart {
    height: 87%;
}

.dity-k {
    height: 21%;
}

#dityKChart {
    height: 87%;
}

/*map*/
.middle {
    float: left;
    width: 45%;
    height: 100%;
}

.chinaMap {
    margin-top: 3%;
    width: 100%;
    height: 70%;
    overflow: hidden;
    z-index: 999999;
    position: relative;
}

.mapBox {
    background: url("../personal-img/旷工.png") no-repeat center;
    background-size: 100% 100%;
    width: 80%;
    margin: 7% auto;
    height: 95%;
}

#map {
    width: 100%;
    height: 100%;
}

.lineRun {
    width: 100%;
    height: 80px;
    background: url("../personal-img/animate.png") no-repeat bottom;
    background-size: 100%;
}

.headTitle {
    font-family: numfont;
    color: #B0CC35;
    position: relative;
    width: 100%;
    text-align: center;
    font-size: 40px;
}

/*right*/
.right {
    float: left;
    width: 25%;
    height: 100%;
    box-sizing: border-box;
    padding-left: 1%; /* Adjust padding to the left instead of right */
    padding-right: 0; /* Remove right padding */
}

.information {
    width: 90%;
    height: 20%;
    padding: 0 5%;
}

.baseInfo,
.temData {
    width: 45%;
    height: 100%;
    float: left;
    background: url("../personal-img/verticalline.png") no-repeat left;
    background-size: auto 100%;
    padding-left: 5%;
}

.infoTitle {
    font-size: 10px;
    color: #AEA7A3;
    margin-top: 0;
}

.area {
    font-family: numfont;
    font-size: 30px;
    color: #B0CC35;
    margin: 0 0 12px 0;
}

.days {
    font-family: numfont;
    color: #B0CC35;
    font-size: 25px;
}

.date {
    display: inline-block;
    text-indent: 29px;
}

.idNum {
    display: inline-block;
    text-indent: 50px;
}

.temTitle {
    color: red;
    font-size: 18px;
    margin: 0;
}

.indoorTem {
    font-family: numfont;
    color: red;
    font-size: 30px;
    margin: 0 0 15px 0;
}

.temperatureN {
    font-size: 60px;
    display: inline-block;
    margin-right: 15px;
}

.point span {
    display: inline-block;

    margin-right: 80px;
}

.wind {
    width: 100%;
    height: 32%; /* 健康画像panel高度 */
    margin-top: 2%;
    margin-bottom: 2%;
}

#windChart {
    width: 45%;
    margin-left: 5%;
    float: left;
    height: 100%;
}

.windData {
    width: 45%;
    float: left;
    height: 100%;
}
.health-row {
    display: flex;
    justify-content: space-between;
}
.windSpeed,
.gust {
    width: 94%;
    height: 40%;
    float: right;
    background: url("../personal-img/line1.png") no-repeat center;
    background-size: 100%;
    overflow: hidden;
}

.windTitle {
    background: #B0CC35;
    width: 60%;
    margin-top: 5%;
    margin-left: 35%;
    height: 14%;
    font-size: 11px;
    font-weight: bold;
    text-align: center;
    line-height: 160%;
}

.windBox {
    width: 100%;
    height: 100%;
    color: #ACA5A1;
}

.windBox>div {
    margin-top: 2%;
    height: 98%;
    float: left;
}

.windBox div p {
    font-size: 12px;
    margin: 5px 0;
}

.windWrap {
    width: 76%;
    height: 80%;
    margin: 20% 12%;
    background: url("../personal-img/wind0.png") no-repeat;
    background-size: 100%;
    overflow: hidden;
}

.windFan {
    width: 60%;
    margin: 20%;
}

.currentSpeed,
.highestSpeed {
    font-family: numfont;
    color: #B0CC35;
    font-size: 30px;
}

.windSpeed .windFan {
    animation: speed 600ms linear infinite;
}

.gust .windFan {
    animation: speed 400ms linear infinite;
}

@keyframes speed {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

/*rain - 基础rainfall样式*/
.rainfall {
    width: 100%;
    height: 33%; /* 恢复默认高度 */
    margin-top: 5%;
    background: linear-gradient(135deg, rgba(0, 21, 41, 0.9), rgba(0, 255, 157, 0.05));
    border: 1px solid rgba(0, 228, 255, 0.3);
    border-radius: 8px;
    backdrop-filter: blur(8px);
    box-shadow: 0 4px 20px rgba(0, 21, 41, 0.3);
}

/* 左侧告警panel专用样式 */
.rainfall.alert-panel {
    height: 30% !important; /* 强制覆盖 */
    margin-top: 3% !important;
    margin-bottom: 2% !important;
}

/* 右侧设备状态panel专用样式 */
.rainfall.device-status {
    height: 28% !important; /* 强制覆盖 */
    margin-top: 5% !important;
    margin-bottom: 3% !important;
}

/* 右侧消息panel专用样式 */
.rainfall.message-panel {
    height: 30% !important; /* 强制覆盖 */
    margin-top: 2% !important;
    margin-bottom: 3% !important;
}

.alert-title {
    text-align: center;
    font-size: 18px; /* 减小字体 */
    margin-bottom: 8px; /* 减少底部边距 */
}
.alert-table table {
    width: 100%;
    border-collapse: collapse;
    margin: 10px 0; /* 减少表格边距 */
    font-size: 11px; /* 减小字体 */
    text-align: left;
}

.alert-table th, .alert-table td {
    padding: 8px; /* 减少内边距 */
    border-bottom: 1px solid #576322;
    color: #adaf2f;
}

.alert-table th {
    background-color: #576322;
    color: white;
}

.alert-table .level {
    padding: 5px 10px;
    border-radius: 5px;
    color: white;
    font-weight: bold;
}

.alert-table .level.medium {
    background-color: #007bff;
}

.alert-table .level.critical {
    background-color: #dc3545;
}

.alert-table .level.high {
    background-color: #ffc107;
}

.alert-table .pending {
    color: red;
}

.alert-table .responded {
    color: green;
}

.message-table table {
    width: 100%;
    border-collapse: collapse;
    margin: 10px 0; /* 减少表格边距 */
    font-size: 11px; /* 减小字体 */
    text-align: left;
}

.message-table th, .message-table td {
    padding: 8px; /* 减少内边距 */
    border-bottom: 1px solid #576322;
    color: #adaf2f;
}

.message-table th {
    background-color: #576322;
    color: white;
}

.message-table .status {
    padding: 5px 10px;
    border-radius: 5px;
    color: white;
    font-weight: bold;
}

.message-table .status.announcement {
    background-color: #007bff;
}

.message-table .status.notification {
    background-color: #6f42c1;
}
.message-table .status.task {
    background-color: #4f9183;
}
.message-table .status.job {
    background-color: #b2abbe;
}
.message-table .status.warning {
    background-color: #dc3545;
}

.message-table .pending {
    color: red;
}

.message-table .responded {
    color: green;
}


.hp_con_right_u{
	background: url(../images/ztl_bg1.png) no-repeat center;
	padding: 2px; /* 减少填充 */
	background-size: 100% 100%;
	margin-bottom: 10px; /* 增加底部边距 */
}

.table3 {
    font-size: 0.16rem;
    border: 1px solid #B0CC35;
    border-top: none;
    margin: 5px auto; /* 减少边距 */
    margin-top: 5%; /* 减少顶部边距 */
}

.table3 tr {
    border-top: 1px solid #B0CC35;
}


.table3 thead tr {
    background: rgba(129, 184, 9, 0.4);
}

.table3 tr.bg-color {
    background: rgba(180, 204, 139, 0.2);
}

.table3 tr td:nth-child(2n+1),
.table3 tr td:nth-child(2n+1)>p {
    width: 3rem;
}

.table3 tr td:nth-child(2n),
.table3 tr td:nth-child(2n)>p {
    width: 1.8rem;
}

.table3 tr td,
.table3 tr th {
    border-right: 1px solid #B0CC35;
    height: 0.4rem;
    line-height: 0.4rem;
    color: #cdddf7;
    text-align: center;
    font-size: 12px;
}

.table3 tr td p {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #cdddf7;
    font-size: 12px;
}

.table3 thead tr th {
    border-right: none;
}

